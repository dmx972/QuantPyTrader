# Production Environment Configuration for QuantPyTrader Frontend

# WebSocket Configuration (will be set by deployment)
REACT_APP_WS_URL=wss://api.quantpytrader.com
REACT_APP_API_URL=https://api.quantpytrader.com

# Disable debug logging
REACT_APP_DEBUG=false
REACT_APP_WS_DEBUG=false

# Production settings
REACT_APP_ENABLE_MOCK_DATA=false
REACT_APP_LOG_LEVEL=error

# Authentication settings (production)
REACT_APP_AUTH_ENABLED=true
REACT_APP_DEMO_MODE=false